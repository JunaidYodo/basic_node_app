{
  "name": "baseapp",
  "version": "1.0.0",
  "description": "server for Base app",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:pull": "dotenv -e .env.development -- npx prisma db pull",
    "migrate_dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate deploy && npx prisma generate",
    "migrate:production": "dotenv -e .env.production -- npx prisma migrate deploy && npx prisma generate",
    "production": "cross-env NODE_ENV=production node --experimental-loader=extensionless server.js",
    "start": "npx prisma migrate deploy && npm run production",
    "dev": "cross-env NODE_ENV=development nodemon --experimental-loader=extensionless server.js",
    "start:dev": "npm run migrate:dev && npm run dev",
    "lint": "eslint .",
    "prettier": "prettier --write \"**/*.{js,json,css,md}\"",
    "format": "prettier --check .",
    "ci-check": "npm run lint && npm run format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "base",
    "app",
    "server"
  ],
  "author": "yodo",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.925.0",
    "@aws-sdk/lib-storage": "^3.925.0",
    "@aws-sdk/s3-request-presigner": "^3.925.0",
    "@prisma/client": "^5.8.1",
    "@sendgrid/mail": "^8.1.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "body-parser": "^2.2.0",
    "cheerio": "^1.1.2",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "ejs": "^3.1.10",
    "escape-html": "^1.0.3",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.2",
    "extensionless": "^1.6.1",
    "form-data": "^4.0.0",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.3.0",
    "js-sha256": "^0.10.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-localstorage": "^3.0.5",
    "nodemailer": "^6.10.1",
    "openai": "^6.8.1",
    "pdf-parse": "^2.4.5",
    "playwright": "^1.56.1",
    "prisma": "^5.8.1",
    "response-time": "^2.3.2",
    "stripe": "^19.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ulid": "^2.3.0",
    "uuid": "^13.0.0",
    "yup": "^1.3.2"
  }
}
